{
  "name": "Warn Before Closing Canvas",
  "version": "1.0",
  "description": "Warn before closing canvas.",
  "content_scripts": [
    {
      "matches": ["http://*.instructure.com/*", "https://*.instructure.com/*"],
      "js": ["content_script.js"]
    }
  ],
  "manifest_version": 2,
  "permissions": ["http://*.instructure.com/*", "https://*.instructure.com/*"]
}
